{
    "name": "4-Colours",
    "type": "dark",
    "tokenColors": [
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#707070"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#da1ac6"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#eb9022"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#F1F1F1"
            }
        },
        {
            "name": "User-defined constant",
            "scope": "constant.character, constant.other",
            "settings": {
                "foreground": "#da1ac6"
            }
        },
        {
            "name": "User-defined constant",
            "scope": "constant.character.escape, constant.other.placeholder, constant.character.format.placeholder.other",
            "settings": {
                "foreground": "#ff0077"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "foreground": "#f1f1f1"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF203B"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF203B"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#02b3e9"
            }
        },
        {
            "name": "Class name",
            "scope": "entity.name.class",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#41ce3c"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#41ce3c"
            }
        },
        {
            "name": "Function Call",
            "scope": "meta.function-call, meta.function-call.c",
            "settings": {
                "foreground": "#f1f1f1"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#41ce3c"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#eb9022"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF203B"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "",
                "foreground": "#41ce3c"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#02b3e9"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#02b3e9"
            }
        },
        {
            "name": "Library class/type",
            "scope": "support.type, support.class",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#02b3e9"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#a0a0a0"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#a0a0a0"
            }
        },
        {
            "name": "JSON String",
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#CFCFC2"
            }
        },
        {
            "name": "diff.header",
            "scope": "meta.diff, meta.diff.header",
            "settings": {
                "foreground": "#75715E"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#FF203B"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#41ce3c"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "#eb9022A0"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "name": "Language methods",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#41ce3c"
            }
        },
        {
            "scope": "heading.1.markdown",
            "settings": {
                "foreground": "#41ce3c",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "punctuation.definition.heading.markdown",
            "settings": {
                "foreground": "#41ce3c"
            }
        },
        {
            "scope": "entity.name.section.markdown",
            "settings": {
                "foreground": "#41ce3c"
            }
        },
        {
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#eb9022"
            }
        },
        {
            "scope": "meta.image.inline.markdown",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "scope": "markup.bold.markdown",
            "settings": {
                "foreground": "#FFFFFF",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.italic.markdown",
            "settings": {
                "foreground": "#FFFFFF",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inline.raw.string.markdown",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#02b3e9"
            }
        },
        {
            "scope": "meta.separator.markdown",
            "settings": {
                "foreground": "#8f8f8f"
            }
        }
    ],
    "colors": {
        //VS Code layout
        //Activity Bar
        "activityBarBadge.background": "#41ce3c",
        "activityBarBadge.foreground": "#202020",
        "activityBar.background": "#1f2127",
        "activityBar.activeBorder": "#fde90e",
        //Status Bar
        "statusBar.background": "#1d1e24",
        "statusBar.debuggingBackground": "#0886da",
        "statusBar.noFolderBackground": "#1f2127",
        //Side Bar
        "sideBar.background": "#262830",
        "sideBarSectionHeader.background": "#2a2d35",
        "sideBarTitle.foreground": "#bbbbbb",
        //Title Bar
        "titleBar.activeBackground": "#1f2127",
        "titleBar.activeForeground": "#ffffff",
        "titleBar.inactiveBackground": "#1f2127",
        "titleBar.inactiveForeground": "#ffffff54",
        "diffEditor.insertedTextBorder": "#41ce3c",
        "diffEditor.removedTextBorder": "#FF203B",
        //Other
        "list.dropBackground": "#0886da",
        "list.focusBackground": "#454856",
        "list.hoverBackground": "#3c3f4d",
		"list.activeSelectionBackground": "#3c3f4d",
        "notifications.background": "#262830",
        "editorWidget.background": "#262830",
        "input.background": "#454856",
        "pickerGroup.foreground": "#87d7ff",
        "dropdown.background": "#262830",
        "debugExceptionWidget.background": "#262830",
        "debugToolBar.background": "#262830",
        "focusBorder": "#454856",
        "textLink.foreground": "#06c1ff",
        //VS Code Editor
        "editor.background": "#1f2127",
        "editor.foreground": "#f1f1f1",
        "editor.selectionHighlightBackground": "#7b98b623",
        "editor.selectionBackground": "#c3d3e450",
        "editor.wordHighlightBackground": "#7b98b623",
        "editor.wordHighlightStrongBackground": "#69829c52",
        "editorGroupHeader.tabsBackground": "#1f2127",
        "editorGutter.addedBackground": "#41ce3c",
        "editorGutter.deletedBackground": "#FF203B",
